    "ejs": "^3.1.10",
    "googleapis": "^148.0.0"
  },
  "devDependencies": {
    "@types/google-spreadsheet": "^4.0.0",
    "@types/node": "^20.17.30",
    "rimraf": "^5.0.5",
    "serverless": "^4.0.0",
    "serverless-esbuild": "^1.50.0",
    "serverless-offline": "^14.4.0",
